.container{
  width:100%;
  background-color: #f2f3f5;
}
.fr{
  float: right;
}
.fl{
  float: left;
}
.c{
  text-align: center
}
.l{
  text-align: left
}
.r{
  text-align: right
}
.container-content{
  padding:20rpx;
}
.service-user,.service-carprot,.service-pay{
  width:100%;
  background-color:#fff;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
}
.clearfix:after{
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.border_top{
  border-top: .5px solid #eee
}
.view_box{
  padding:25rpx 0;
}
.service-user-title{
  font-size: 38rpx;
  font-weight: 600;
  color:#000;
  padding:10rpx 0;
}
.service-user-common{
  margin:0 10rpx;
}
.type-1{
  font-size:26rpx;
  color:#7a6f78;

}
.phClass{
  color: #bdbebd;
  font-size:26rpx;
}
.red{
  color:red
}
.text-l{
  float:left;
  display: flex;
  font-size:26rpx;
  color:#666;
  align-items: center;
}
.text-r{
  float:right;
  color:#666;
  font-size:26rpx;
}
.sibmitPay{
  width: 100%;
  background-color: #333333;
  color: white;
  height: 80rpx;
  font-size: 30rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 6rpx;
  margin-top: 80rpx;
  margin-bottom: 10rpx;

}

